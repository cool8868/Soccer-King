<UserControl x:Class="Games.NB.Match.Emulator.WPF.Player"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="20" Width="20">
    <Canvas Background="Transparent">
        <Ellipse Width="20" Height="20" Name="ellipsePlayer" Stroke="Black" />
        <Line Name="linePlayer" X1="9" Y1="10" X2="19" Y2="10" Stroke="Black" RenderTransformOrigin="0.5,1">
            <Line.RenderTransform>
                <TransformGroup>
                    <RotateTransform x:Name="R1" Angle="0"></RotateTransform>
                </TransformGroup>
            </Line.RenderTransform>
        </Line>
        <Label x:Name="position" Height="25.083" Content="Unk" Canvas.Left="-5.339" Canvas.Top="11.583" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        <Label x:Name="playername" Visibility="Hidden" Height="25.083" Content="Unk" Canvas.Left="15.339" Canvas.Top="11.583" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        <Label x:Name="skill" Height="25" Content="" Canvas.Left="-19" Canvas.Top="-20" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        <Label x:Name="state" Height="25" Content="" Canvas.Left="17" Canvas.Top="0" />
    </Canvas>    
</UserControl>
