#ifndef __IDECREASEDEBUFFRATESTATUS_H__
#define __IDECREASEDEBUFFRATESTATUS_H__

/// 表示了减免异常效果的状态
class IDecreaseDebuffRateStatus 
{
public:

    virtual ~IDecreaseDebuffRateStatus() {}

public:

    /// Current rate.
    virtual double      Rate() = 0;
    virtual void        SetRate(double rate) = 0;

    /// Raw rate.
    virtual double      RawRate() = 0;
    virtual void        SetRawRate(double rate) = 0;
};

#endif //__IDECREASEDEBUFFRATESTATUS_H__
