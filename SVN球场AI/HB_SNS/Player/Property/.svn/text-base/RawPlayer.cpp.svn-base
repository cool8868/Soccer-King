#include "RawPlayer.h"

RawPlayer::RawPlayer()
{
    InitVariables();
}

RawPlayer::~RawPlayer() 
{
    DeletePtrSafe(m_BuildinAttribute);
}

void RawPlayer::InitVariables()
{
    m_Id                = 0;
    m_BuildinAttribute  = NULL;

    m_RawProperty.clear();
    m_Skills.clear();
}


